{
  "name": "alaska-cli",
  "version": "0.1.0",
  "description": "Alaska command line interface",
  "engines": {
    "node": ">=5.0.0"
  },
  "bin": {
    "alaska": "bin/alaska"
  },
  "scripts": {
    "build": "babel ./src --out-dir ./lib",
    "watch": "babel ./src --out-dir ./lib -w",
    "prebuild": "rimraf lib",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maichong/alaska-cli.git"
  },
  "author": {
    "email": "liang@maichong.it",
    "name": "liang",
    "url": "https://github.com/liangxingchen"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maichong/alaska-cli/issues"
  },
  "homepage": "https://github.com/maichong/alaska-cli#readme",
  "peerDependencies": {
    "alaska": "*"
  },
  "devDependencies": {
    "babel-plugin-syntax-async-functions": "^6.5.0",
    "babel-plugin-syntax-class-properties": "^6.5.0",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-es2015-destructuring": "^6.6.0",
    "babel-plugin-transform-es2015-parameters": "^6.6.0",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "rimraf": "^2.5.2"
  },
  "dependencies": {
    "babel-plugin-transform-async-to-generator": "^6.5.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.6.3",
    "commander": "^2.9.0"
  }
}
